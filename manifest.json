{
    "name": "ChtickyNotes",
    "id": "chtickynotes",
    "packaging_format": 1,
    "description": {
        "en": "Generic sticky notes for all purpose",
        "fr": "Post-its génériques à tout faire"
    },
    "version": "1.0~ynh4",
    "url": "https://github.com/YunoHost-Apps/chtickynotes_ynh/",
    "upstream": {
        "license": "MIT",
        "code": "https://github.com/YunoHost-Apps/chtickynotes_ynh/"
    },
    "developer": {
        "name": "chtixof"
    },
    "license": "MIT",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/chtickynotes",
                "default": "/chtickynotes"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
